{
   "version": 2,
   "routes": [
      {
         "src": "/fonts/(.*)",
         "headers": {
            "cache-control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/assets/fonts/(.*)",
         "headers": {
            "cache-control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
}